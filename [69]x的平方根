# leetcode submit region begin(Prohibit modification and deletion)
class Solution(object):
    def mySqrt(self, x):
        """
        :type x: int
        :rtype: int
        """
        if not x:
            return 0
        if x == 1:
            return 1
        minc, maxc = 0, x-1
        while minc <= maxc:
            mid = (minc+maxc)//2
            if mid*mid >x:
                maxc = mid - 1
                mid -= 1
            if mid*mid <x:
                minc = mid + 1
            if mid*mid == x:
                return mid
        return mid

solution = Solution()
print(solution.mySqrt(8))

